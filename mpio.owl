    <!-- http://purl.obolibrary.org/obo/OBI_1110060 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/OBI_1110060">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/RO_0002087"/>
        <obo:IAO_0000111 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">process is result of</obo:IAO_0000111>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The production of IFN-gamma by effector T cells is a process result of T cell stimulation through the TCR</obo:IAO_0000112>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000123"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is a relationship between a process and a preceding occurrent that directly caused the later one to occur</obo:IAO_0000115>
